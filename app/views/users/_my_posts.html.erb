<h1>Your posts</h1>
<div class="col-sm-12 col-md-12 col-lg-12">
    <table class="table">
      <thead>
          <tr>
            <th>Title</th>
            <th class="text-right">Date</th>
          </tr>
      </thead>
      <tbody>
          <% @posts.each do |post| %>
          <tr>
              <td scope="row"><%= link_to post.title, post_url(post.id) %></td>
              <td class="text-right"><%=post.created_at.strftime("%d/%m/%Y")%></td>
          </tr>
          <% end %>
      </tbody>
  </table>
  </div>